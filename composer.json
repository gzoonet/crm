{
    "name": "gzoonet/crm",
    "description": "A modular CRM plugin for Laravel + Filament",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "GZOO Media",
            "email": "eddy@gzoo.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.15",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Gzoonet\\Crm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gzoonet\\Crm\\CrmPackageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
